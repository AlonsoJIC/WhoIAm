{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/portafolio/browser",
  "framework": "angular",
  "functions": {
    "api/**/*.ts": {
      "runtime": "@vercel/node@3.0.0",
      "maxDuration": 10
    }
  },
  "env": {
    "NODE_VERSION": "18"
  },
  "routes": [
    {
      "src": "/r/(.*)",
      "dest": "/api/track/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
