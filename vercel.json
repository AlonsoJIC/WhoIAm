{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/portafolio/browser",
  "framework": "angular",
  "functions": {
    "api/**/*.ts": {
      "runtime": "@vercel/node@3.0.0"
    }
  },
  "rewrites": [
    {
      "source": "/r/:id",
      "destination": "/api/track/:id"
    },
    {
      "source": "/((?!api|r/).*)",
      "destination": "/index.html"
    }
  ]
}
